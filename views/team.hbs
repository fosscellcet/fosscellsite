                                <img src="images/{{member.image}}" class="card-img-top" alt="{{member.name}}">
                                <div class="card-body">
                                    <h4 class="card-title">{{member.name}}</h4>
                                    <h5 class="card-text">{{member.designation}}</h5>
                                    <blockquote class=" card-text blockquote font-italic">"{{member.quote}}"</blockquote>     
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="card-deck">
                <div class="card">
                    <div class=" card-header">
                        <h4 class="card-title">Technical Team</h4>
                    </div>
                    <div class="card-body">
                        <div class="card-deck">
                            {{#each members.tech as |member|}}
                                <div class="card">
                                    <img src="images/{{member.image}}" class="card-img-top" alt="{{member.name}}">
                                    <div class="card-body">
                                        <h4 class="card-title">{{member.name}}</h4>
                                        <h5 class="card-text">{{member.designation}}</h5>
                                        <blockquote class=" card-text blockquote font-italic">"{{member.quote}}"</blockquote>     
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <div class="card">
                    <div class=" card-header">
                        <h4 class="card-title">Project Team</h4>
                    </div>
                    <div class="card-body">
                        <div class="card-deck">
                            {{#each members.project as |member|}}
                                <div class="card">
                                    <img src="images/{{member.image}}" class="card-img-top" alt="{{member.name}}">
                                    <div class="card-body">
                                        <h4 class="card-title">{{member.name}}</h4>
                                        <h5 class="card-text">{{member.designation}}</h5>
                                        <blockquote class=" card-text blockquote font-italic">"{{member.quote}}"</blockquote>     
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="card">
                <div class=" card-header">
                    <h4 class="card-title">Membership Team</h4>
                </div>
                <div class="card-body">
