    <div class="container-fluid">
        <h2>Main Execom</h2>
        <div class="row">
            {{#each members.main as |member|}}
                <div class="col-md-2">
                    <img src="images/{{member.image}}" class="img-fluid" alt="{{member.name}}">
                    <h4>{{member.name}}</h3>
                    <h5>{{member.designation}}</h6>
                    <blockquote class="blockquote font-italic">"{{member.quote}}"</blockquote>     
